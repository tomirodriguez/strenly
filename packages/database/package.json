{
  "name": "@strenly/database",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schema/index.ts",
    "./schema": "./src/schema/index.ts",
    "./client": "./src/client.ts"
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "format": "pnpm biome format --write src",
    "lint": "pnpm biome check --write src",
    "lint:fix": "pnpm biome check --write --unsafe src",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:drop": "drizzle-kit drop",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:plans": "tsx scripts/seed-plans.ts"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.7",
    "postgres": "^3.4.8"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.2",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3"
  }
}